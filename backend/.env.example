# VPNSpan Environment Variables
# Copy this file to .env and fill in your values

# Server Configuration
NODE_ENV=production
PORT=5000

# Surfshark VPN Configuration (Optional)
# Get your credentials from https://my.surfshark.com/vpn/manual-setup/main
SURFSHARK_USER=your_surfshark_username
SURFSHARK_PASS=your_surfshark_password
SURFSHARK_OVPN_PATH=/etc/openvpn/us-lax.prod.surfshark.com_tcp.ovpn

# ProtonVPN Configuration (Optional)
# Get your credentials from https://account.protonvpn.com/downloads
PROTONVPN_USER=your_protonvpn_username
PROTONVPN_PASS=your_protonvpn_password
PROTONVPN_OVPN_PATH=/etc/openvpn/protonvpn/us-free-110.protonvpn.tcp.ovpn
